{
  "name": "pisethchhun/bakong-khqr-php",
  "description": "PHP package for generating and parsing Bakong KHQR (Cambodian QR payment) codes",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "bakong",
    "khqr",
    "cambodia",
    "qr-code",
    "payment",
    "php"
  ],
  "authors": [
    {
      "name": "Piseth",
      "email": "chhunpiseth.mail@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.10",
    "pestphp/pest": "^3.8",
    "pestphp/pest-plugin-drift": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Piseth\\BakongKhqr\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Piseth\\BakongKhqr\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "pest",
    "test:unit": "pest tests/Unit",
    "test:feature": "pest tests/Feature",
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "analyse": "phpstan analyse"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
